{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "ES2022",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"allowJs": false,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"skipLibCheck": true
	},
	"exclude": [
		"dist"
	]
}